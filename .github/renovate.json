{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "docker"
      ],
      "matchPackageNames": [
        "homebridge/homebridge"
      ],
      "versioning": "loose",
      "allowedVersions": "/^\\d{4}-\\d{2}-\\d{2}$/"
    }
  ]
}
